page{
  background: #f0f0f0;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
}
.list-box{
  padding: 26rpx 30rpx;
  border-radius: 10px;
  background: #fff;
  font-size: 28rpx;
  margin: 20rpx 0;
  display: flex;
  align-items: flex-start;
}

.images-box{
  width: 150rpx;
  height: 150rpx;
  overflow: hidden;
}
.images-box>image{
  width: 100%;
  height: 100%;
}
.list-boc-right{
  flex: 1;
  padding-left: 22rpx;
}

.item-title{
  font-size: 32rpx;
  font-weight: 600;
  overflow:hidden; /*超出的文本隐藏*/
  text-overflow:ellipsis; /*溢出用省略号显示*/
  white-space:nowrap; /*溢出不换行*/
}